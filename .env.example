# Application Settings
APP_NAME=sales-agents-whitelabel
APP_ENV=development
LOG_LEVEL=INFO

# Strands Agents Configuration
STRANDS_MODEL_PROVIDER=openai
STRANDS_MODEL_NAME=gpt-4
STRANDS_API_KEY=
STRANDS_TEMPERATURE=0.7
STRANDS_MAX_TOKENS=2000

# MCP Server Configuration
MCP_CRM_SERVER_URL=http://localhost:8001
MCP_CATALOG_SERVER_URL=http://localhost:8002
MCP_ANALYTICS_SERVER_URL=http://localhost:8003

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=1.0
RETRY_BACKOFF=2.0

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600

# Metrics Configuration
METRICS_ENABLED=true
METRICS_PORT=9090
